{
    "family": "jenkins-task-gigyaauth",
    "containerDefinitions": [
        {
            "image": "149324216444.dkr.ecr.us-east-1.amazonaws.com/gigyaauth-test-repo",
            "name": "jenkins-task-gigyaaut",
            "cpu": 10,
            "memory": 256,
            "essential": true,
            "networkMode" : "host",
            "portMappings": [
                {
                    "containerPort": 8082,
                    "hostPort": 8082
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "awslogs-ecs",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "log-gigya-auth"
                }
            }
        }
    ]
}
