{
    "family": "jenkins-task-gigyaservice",
    "containerDefinitions": [
        {
            "image": "149324216444.dkr.ecr.us-east-1.amazonaws.com/gigyaservice-test-repo",
            "name": "jenkins-task-gigyaservice",
            "cpu": 10,
            "memory": 256,
            "essential": true,
            "networkMode" : "host",
            "portMappings": [
                {
                    "containerPort": 8081,
                    "hostPort": 8081
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "awslogs-ecs",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "awslogs-logging"
                }
            }
        }
    ]
}
